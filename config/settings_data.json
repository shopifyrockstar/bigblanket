      "story-banner": {
        "type": "story-banner",
        "settings": {
          "image": "shopify:\/\/shop_images\/hero-desktop_ab612e3e-db7b-41bb-836c-bc7d249bcf1f.jpg",
          "mobile_image": "shopify:\/\/shop_images\/hero-mobile_a3ae46e8-4917-4159-b86f-965439ff2813.jpg",
