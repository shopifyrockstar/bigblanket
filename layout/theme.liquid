  <script>
    (function () {
      window.DY = window.DY || {};
      window.DY.pageTypes = {
        other: 'OTHER',
        homepage: 'HOMEPAGE',
        category: 'CATEGORY',
        product: 'PRODUCT',
        cart: 'CART'
      };      
      var pageType;      
      
      {% if template contains 'index' %}
      	pageType = window.DY.pageTypes.homepage;
      {% elsif template contains 'collection' %}
      	pageType = window.DY.pageTypes.category;
      {% elsif template contains 'product' %}
      	pageType = window.DY.pageTypes.product;
      {% elsif template contains 'cart' %}
      	pageType = window.DY.pageTypes.cart;
      {% else %}
      	pageType = window.DY.pageTypes.other;
      {% endif %}
      
      var data;
      
      {% if template contains 'index' %}
      	//silence is gold
      {% elsif template contains 'collection' %}
      
      	data = ['{{ collection.handle }}'];
                
      {% elsif template contains 'product' %}
                
      	data = [];      	        
        var final_data = '{{ product.selected_or_first_available_variant.sku }}' + "-" + {{ product.selected_or_first_available_variant.id }};      	
      	data.push(final_data);
      
      {% elsif template contains 'cart' %}
      	data = [];
      	{% for item in cart.items %}
      		var final_data = '{{ item.sku }}' + "-" + '{{ item.variant.id }}';
      		data.push(final_data);
      	{% endfor %}
      
      {% else %}
      	//silence is gold
      {% endif %}
      
      window.DY.recommendationContext = { type: pageType };

      if (data) {
        window.DY.recommendationContext.data = data;
      }
    })();
  </script>
  
